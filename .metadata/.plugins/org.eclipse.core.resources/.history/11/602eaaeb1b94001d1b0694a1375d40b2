
public class VariablesConstantes {
	public static void main(String[] args) {

		// declare la variable goles
		byte goles = 4;	
		System.out.println(goles);
		
		goles = 8;
		System.out.println(goles);
		
		byte faltas = 190;
		System.out.println(faltas);
		
		short edad;
		
		int entero1;
		int entero2;
		int enetro3;
		
		long lon1,long2;
		
		
		
	}
}
