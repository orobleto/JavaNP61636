
public class OperadoresAsignacion {
	public static void main(String[] args) {
		// int numero1, numero2 = 3;
		// int numero1 = 5, numero2 = 3;
		int numero1, numero2;
		double total;

		numero1 = 5;
		numero2 = 6;
		total = numero1 + numero2;
		System.out.println(total);
		
		total = total + 1; 
		System.out.println(total);
		
		total+=25;
		System.out.println(total);
		

	}
}
